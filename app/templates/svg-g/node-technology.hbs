<ellipse class="node_tech" cx="{{unbound node.cx}}"
                           cy="{{unbound node.cy}}" 
                           rx="{{unbound node.rx}}" 
                           ry="{{unbound node.ry}}"></ellipse>

<text class="node_txt" x="{{unbound node.cx}}" y="{{unbound node.y_padded}}">
  <tspan dy="32">
    {{node.name}}
  </tspan>
  <tspan x="{{unbound node.cx}}" dy="16.8">
    {{node.year}}
  </tspan>
</text>
