<rect class="node_header" x="{{unbound node.x_padded}}" 
                          y="{{unbound node.y_padded}}" 
                          height="{{unbound node.height}}"
                          width="{{unbound node.width}}"></rect>

<text class="node_header_title" x="{{unbound node.cx}}" y="{{unbound node.cy}}">
  <tspan>
    {{node.title}}
  </tspan>
</text>
